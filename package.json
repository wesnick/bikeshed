{
  "dependencies": {
  },
  "scripts": {
    "build-bulma": "sass --load-path=node_modules style/app.scss build/css/app.css",
    "build-js": "vite build",
    "dev": "concurrently \"sass --load-path=node_modules style/app.scss build/css/app.css --watch\" \"vite\"",
    "build": "sass --load-path=node_modules style/app.scss build/css/app.css && vite build",
    "start": "npm run dev"
  },
  "devDependencies": {
    "@vizuaalog/bulmajs": "^0.12.2",
    "bulma": "^1.0.3",
    "concurrently": "^8.2.0",
    "sass": "^1.85.1",
    "vite": "^6.2.0"
  }
}
